<script>
    $('.show-reports').click(function(){
        if( $('#from_date').val() == '' || $('#from_date').val() == [] ) alert('Choose From Date')
        else if ( $('#to_date').val() == '' || $('#to_date').val() == [] ) alert('Choose To Date')
        else {
            let type = $('#type').val()
            let from_date = $('#from_date').val()
            let to_date = $('#to_date').val()

            $.get('/admin/dashboard/transaction/reports/bytype',{type,from_date,to_date},data=>{
                console.log(data)
                makeTable(data)
            })
        }
    })


 


    function makeTable(data){
    let table = ` <div class="table-responsive">

    <button type="button"  class="btn btn-primary" style="margin:20px">Total Transaction Recieved : ${data[0][0].total_amount} </button>
<table id="report-table" class="table  table-striped mb-0">
<thead>
<tr>
<th>Order Id</th>
<th>Amount</th>
<th>Date</th>
<th>Number</th>
<th>Name</th>

</tr>
</thead>
<tbody>`

$.each(data[1],(i,item)=>{
table+=`<tr>

<td>${item.orderid}</td>
<td>${item.amount}</td>
<td>${item.date}</td>
<td>${item.number}</td>
<td>${item.username}</td>

</tr>`
})
table+=`</tbody>
</table>
</div>

  
<!-- End Row -->`
    $('#result').html(table)
    $('#result').show()
}

</script>



