                <div class="row">
                  
                    <div class="col-xl-6 col-md-6 col-xxl-6">
                        <div class="card">
                            <div class="card-body">
                                <div class="media align-items-center mb-4">
                                   
                                    <div class="media-body">
                                        <h5 class="mb-0"><%=result[0][0].name%> (Rs. <%=result[0][0].price%>) </h5>
                                       
                                    </div>
                                 
                                    
                                     
                                </div>

                                <h4>Vendor's Details</h4>
                                <p class="text-muted"><%=result[0][0].vendorname%> (<%=result[0][0].vendornumber%>)</p>
                                <ul class="card-profile__info">
                                    <li><strong class="text-dark mr-4">Category</strong> <span><%=result[0][0].categoryname%></span></li>
                                    <li><strong class="text-dark mr-4">SubCategory</strong> <span><%=result[0][0].subcategoryname%></span></li>
                                   
                                </ul>

                             
                            </div>
                          
                        </div>
                    </div>
                    <div class="col-xl-6 col-md-6 col-xxl-6">
                        <div class="card">
                            <div class="card-body">
                                <div id="activity" class="py-2">
                                   
                                    <div class="media border-bottom py-3">
                                        <img width="45" src="/images/<%=result[0][0].image%>" alt="avatar" class="mr-3">
                                        <img width="45" src="/images/<%=result[0][0].thumbnail%>" alt="avatar" class="mr-3">


                                        
                                       

<% for(i=0;i<result[1].length;i++){ %>
    <img width="45" src="/images/<%=result[1][i].image%>" alt="avatar" class="mr-3">
 
<% } %>

                                    </div>



                                   


                                    <% if(result[0][0].status == 'approved' || result[0][0].status == 'rejected') { %>
                                   
                                 


                             <% } else { %>
                                <input type="text" class="form-control" placeholder="Remarks ( If you reject this product )"><br><br/>

                                <div class="media py-3">


                                    <button type="button" class='btn btn-success change_status' name="approved"  id="<%=result[0][0].id%>">Approved</button>
                                    <div class="media-body">
                                      
                                        <button type="button" class='btn btn-warning change_status' name="rejected" id="<%=result[0][0].id%>" style="margin-left: 20px;">Reject</button>
                                        
                                    </div>
                                </div>


                                <% } %>


                                </div>
                            </div>
                        </div>
                    </div>
                  
                </div>
            </div>
